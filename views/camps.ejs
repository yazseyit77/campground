<%- include("partials/header") -%>

<div class="row text-center">
    <% camps.forEach(camp => { %>
    <div class="col-md-6 col-sm-12" style="display: flex; flex-wrap: wrap;">
        <div class="card">
            <img class="img-thumbnail" style="height: 400px; width: 100%;" src="<%= camp.img %>" alt="">
            <div class="card-body">
                <h3><%= camp.name %></h3>
            </div>
        </div>
    </div>
    <% }) %>
</div>

<%- include("partials/footer") -%>